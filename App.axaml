<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="using:SdkMigrator.Converters"
             x:Class="SdkMigrator.App"
             RequestedThemeVariant="Light">
    
    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Fluent.xaml"/>
        
        <!-- Custom Styles for Modern Look -->
        <Style Selector="Window">
            <Setter Property="Background" Value="#FAFAFA"/>
        </Style>
        
        <Style Selector="Button">
            <Setter Property="Padding" Value="12,8"/>
            <Setter Property="CornerRadius" Value="6"/>
            <Setter Property="FontWeight" Value="Medium"/>
            <Setter Property="Transitions">
                <Transitions>
                    <BrushTransition Property="Background" Duration="0:0:0.2"/>
                </Transitions>
            </Setter>
        </Style>
        
        <Style Selector="Button:pointerover">
            <Setter Property="Background" Value="#E8E8E8"/>
        </Style>
        
        <Style Selector="Button.Primary">
            <Setter Property="Background" Value="#0EA5E9"/>
            <Setter Property="Foreground" Value="White"/>
        </Style>
        
        <Style Selector="Button.Primary:pointerover">
            <Setter Property="Background" Value="#0284C7"/>
        </Style>
        
        <Style Selector="Button.Success">
            <Setter Property="Background" Value="#10B981"/>
            <Setter Property="Foreground" Value="White"/>
        </Style>
        
        <Style Selector="Button.Success:pointerover">
            <Setter Property="Background" Value="#059669"/>
        </Style>
        
        <Style Selector="Button.Warning">
            <Setter Property="Background" Value="#F59E0B"/>
            <Setter Property="Foreground" Value="White"/>
        </Style>
        
        <Style Selector="Button.Warning:pointerover">
            <Setter Property="Background" Value="#D97706"/>
        </Style>
        
        <Style Selector="TextBox">
            <Setter Property="Padding" Value="10,8"/>
            <Setter Property="CornerRadius" Value="6"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="BorderBrush" Value="#E5E5E5"/>
        </Style>
        
        <Style Selector="TextBox:focus">
            <Setter Property="BorderBrush" Value="#0EA5E9"/>
        </Style>
        
        <Style Selector="ComboBox">
            <Setter Property="Padding" Value="10,8"/>
            <Setter Property="CornerRadius" Value="6"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="BorderBrush" Value="#E5E5E5"/>
        </Style>
        
        <Style Selector="TabControl">
            <Setter Property="Background" Value="Transparent"/>
        </Style>
        
        <Style Selector="TabItem">
            <Setter Property="Padding" Value="16,12"/>
            <Setter Property="Margin" Value="0,0,8,0"/>
            <Setter Property="FontWeight" Value="Medium"/>
        </Style>
        
        <Style Selector="TabItem:selected">
            <Setter Property="Background" Value="White"/>
            <Setter Property="Foreground" Value="#0EA5E9"/>
        </Style>
        
        <Style Selector="Border.Card">
            <Setter Property="Background" Value="White"/>
            <Setter Property="CornerRadius" Value="8"/>
            <Setter Property="BoxShadow" Value="0 1 3 0 #00000014"/>
            <Setter Property="Padding" Value="20"/>
            <Setter Property="Margin" Value="0,0,0,12"/>
        </Style>
        
        <Style Selector="TextBlock.SectionHeader">
            <Setter Property="FontSize" Value="18"/>
            <Setter Property="FontWeight" Value="SemiBold"/>
            <Setter Property="Margin" Value="0,0,0,16"/>
            <Setter Property="Foreground" Value="#1F2937"/>
        </Style>
        
        <Style Selector="TextBlock.Label">
            <Setter Property="FontSize" Value="14"/>
            <Setter Property="FontWeight" Value="Medium"/>
            <Setter Property="Margin" Value="0,0,0,6"/>
            <Setter Property="Foreground" Value="#374151"/>
        </Style>
        
        <Style Selector="TextBlock.HelpText">
            <Setter Property="FontSize" Value="12"/>
            <Setter Property="Foreground" Value="#6B7280"/>
            <Setter Property="Margin" Value="0,4,0,0"/>
        </Style>
        
        <Style Selector="ProgressBar">
            <Setter Property="Height" Value="6"/>
            <Setter Property="CornerRadius" Value="3"/>
            <Setter Property="Background" Value="#E5E7EB"/>
            <Setter Property="Foreground" Value="#0EA5E9"/>
        </Style>
        
        <Style Selector="CheckBox">
            <Setter Property="Padding" Value="8,4"/>
        </Style>
        
        <Style Selector="DataGrid">
            <Setter Property="GridLinesVisibility" Value="Horizontal"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="BorderBrush" Value="#E5E5E5"/>
            <Setter Property="RowBackground" Value="White"/>
        </Style>
        
        <!-- Additional Styles -->
        <Style Selector="TextBlock.PageTitle">
            <Setter Property="FontSize" Value="24"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="Foreground" Value="#1F2937"/>
        </Style>
        
        <Style Selector="TextBlock.SectionTitle">
            <Setter Property="FontSize" Value="16"/>
            <Setter Property="FontWeight" Value="SemiBold"/>
            <Setter Property="Foreground" Value="#374151"/>
        </Style>
        
        <Style Selector="Grid.InputGroup">
            <Setter Property="Margin" Value="0,8,0,0"/>
        </Style>
        
        <Style Selector="TextBox.ModernTextBox">
            <Setter Property="Padding" Value="10,8"/>
            <Setter Property="CornerRadius" Value="6"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="BorderBrush" Value="#E5E7EB"/>
        </Style>
        
        <Style Selector="Button.PrimaryButton">
            <Setter Property="Background" Value="#0EA5E9"/>
            <Setter Property="Foreground" Value="White"/>
            <Setter Property="Padding" Value="12,8"/>
            <Setter Property="CornerRadius" Value="6"/>
            <Setter Property="FontWeight" Value="Medium"/>
        </Style>
        
        <Style Selector="Button.PrimaryButton:pointerover">
            <Setter Property="Background" Value="#0284C7"/>
        </Style>
        
        <Style Selector="Button.SecondaryButton">
            <Setter Property="Background" Value="#F3F4F6"/>
            <Setter Property="Foreground" Value="#374151"/>
            <Setter Property="Padding" Value="12,8"/>
            <Setter Property="CornerRadius" Value="6"/>
            <Setter Property="FontWeight" Value="Medium"/>
        </Style>
        
        <Style Selector="Button.SecondaryButton:pointerover">
            <Setter Property="Background" Value="#E5E7EB"/>
        </Style>
        
        <Style Selector="Button.LinkButton">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="Foreground" Value="#0EA5E9"/>
            <Setter Property="Padding" Value="4,2"/>
            <Setter Property="FontWeight" Value="Medium"/>
            <Setter Property="Cursor" Value="Hand"/>
        </Style>
        
        <Style Selector="Button.LinkButton:pointerover">
            <Setter Property="Foreground" Value="#0284C7"/>
        </Style>
        
        <Style Selector="Border.SubCard">
            <Setter Property="Background" Value="#F9FAFB"/>
            <Setter Property="CornerRadius" Value="6"/>
            <Setter Property="BorderBrush" Value="#E5E7EB"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Padding" Value="16"/>
        </Style>
        
        <Style Selector="Border.SelectableItem">
            <Setter Property="Background" Value="Transparent"/>
            <Setter Property="CornerRadius" Value="4"/>
        </Style>
        
        <Style Selector="Border.SelectableItem:pointerover">
            <Setter Property="Background" Value="#F3F4F6"/>
        </Style>
        
        <Style Selector="ListBoxItem:selected Border.SelectableItem">
            <Setter Property="Background" Value="#EFF6FF"/>
        </Style>
    </Application.Styles>
    
    <Application.Resources>
        <SolidColorBrush x:Key="PrimaryColor">#0EA5E9</SolidColorBrush>
        <SolidColorBrush x:Key="SecondaryColor">#6366F1</SolidColorBrush>
        <SolidColorBrush x:Key="SuccessColor">#10B981</SolidColorBrush>
        <SolidColorBrush x:Key="WarningColor">#F59E0B</SolidColorBrush>
        <SolidColorBrush x:Key="ErrorColor">#EF4444</SolidColorBrush>
        <SolidColorBrush x:Key="BackgroundColor">#FAFAFA</SolidColorBrush>
        <SolidColorBrush x:Key="CardBackground">White</SolidColorBrush>
        
        <!-- Converters -->
        <converters:RiskLevelToColorConverter x:Key="RiskLevelToColorConverter"/>
        <converters:FileNameConverter x:Key="FileNameConverter"/>
        
